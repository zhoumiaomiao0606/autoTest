    <!-- countListBizModelByPartnerId -->
    <select id="countListBizModelByPartnerId" parameterType="com.yunche.loan.domain.query.BizModelQuery"
            resultType="java.lang.Integer">
        SELECT COUNT(a.biz_id)
        FROM biz_model_rela_area_partners AS a
        JOIN biz_model AS b
        ON a.`biz_id` = b.`biz_id`
        <if test="id != null">
            AND a.group_id = #{id}
        </if>
        <if test="cascadeAreaIdList != null and cascadeAreaIdList.size() > 0">
            AND a.area_id IN
            <foreach collection="cascadeAreaIdList" item="item" open="(" close=")" separator=",">
                #{item}
            </foreach>
        </if>
        <if test="status != null">
            AND b.status = #{status}
        </if>
    </select>

    <!-- listBizModelByPartnerId -->
    <select id="listBizModelByPartnerId" parameterType="com.yunche.loan.domain.query.BizModelQuery"
            resultMap="BaseResultMap">
        SELECT b.*
        FROM biz_model_rela_area_partners AS a
        JOIN biz_model AS b
        ON a.`biz_id` = b.`biz_id`
        <if test="id != null">
            AND a.group_id = #{id}
        </if>
        <if test="cascadeAreaIdList != null and cascadeAreaIdList.size() > 0">
            AND a.area_id IN
            <foreach collection="cascadeAreaIdList" item="item" open="(" close=")" separator=",">
                #{item}
            </foreach>
        </if>
        <if test="status != null">
            AND b.status = #{status}
        </if>
        ORDER BY b.`biz_id` ASC
        LIMIT #{startRow}, #{pageSize}
    </select>

    <!--selectByCondition-->
    <select id="selectByCondition" parameterType="com.yunche.loan.domain.query.BizModelQuery"
            resultMap="BaseResultMap">
        select
        biz_model.*
        from biz_model
        LEFT JOIN biz_model_rela_area_partners
        ON biz_model.biz_id = biz_model_rela_area_partners.biz_id
        where 1=1
        <if test="title != null">
            AND
            title LIKE '%${title}%'
        </if>
        <if test="scene != null">
            AND
            scene = #{scene,jdbcType=VARCHAR}
        </if>
        <if test="custTarget != null">
            AND
            cust_target = #{custTarget,jdbcType=VARCHAR}
        </if>
        <if test="carType != null">
            AND
            <if test="carType == 2">
                car_type IN (0, 1, 2)
            </if>
            <if test="carType != 2">
                car_type = #{carType,jdbcType=TINYINT}
            </if>
        </if>
        <if test="cascadeAreaIdList != null">
            AND
            biz_model_rela_area_partners.area_id IN
            <foreach item="item" index="index" collection="cascadeAreaIdList"
                     open="(" separator="," close=")">
                #{item}
            </foreach>
        </if>
        <if test="status != null">
            AND status = #{status}
        </if>
    </select>

