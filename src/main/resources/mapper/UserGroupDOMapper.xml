<mapper namespace="com.yunche.loan.mapper.UserGroupDOMapper">
    <resultMap id="BaseResultMap" type="com.yunche.loan.domain.entity.UserGroupDO">
        <id column="id" jdbcType="BIGINT" property="id"/>
        <result column="name" jdbcType="VARCHAR" property="name"/>
        <result column="department_id" jdbcType="BIGINT" property="departmentId"/>
        <result column="gmt_create" jdbcType="TIMESTAMP" property="gmtCreate"/>
        <result column="gmt_modify" jdbcType="TIMESTAMP" property="gmtModify"/>
        <result column="feature" jdbcType="VARCHAR" property="feature"/>
        <result column="status" jdbcType="TINYINT" property="status"/>
    </resultMap>
    <resultMap extends="BaseResultMap" id="ResultMapWithBLOBs" type="com.yunche.loan.domain.entity.UserGroupDO">
        <result column="info" jdbcType="LONGVARCHAR" property="info"/>
    </resultMap>

    <sql id="Base_Column_List">
        id, name, department_id, gmt_create, gmt_modify, feature, status
    </sql>
    <sql id="Blob_Column_List">
        info
    </sql>

    <sql id="Base_Query_List">
        <if test="name != null and name != '' ">
            AND name LIKE CONCAT('%',#{name},'%')
        </if>
        <if test="gmtCreateStart != null">
            <![CDATA[ AND gmt_create >= #{gmtCreateStart} ]]>
        </if>
        <if test="gmtCreateEnd != null">
            <![CDATA[ AND gmt_create <= #{gmtCreateEnd} ]]>
        </if>
        <if test="gmtModifyStart != null">
            <![CDATA[ AND gmt_modify >= #{gmtModifyStart} ]]>
        </if>
        <if test="gmtModifyEnd != null">
            <![CDATA[ AND gmt_modify <= #{gmtModifyEnd} ]]>
        </if>
        <if test="status != null">
            AND status = #{status}
        </if>
    </sql>

    <!--  count -->
    <select id="count" parameterType="com.yunche.loan.domain.query.UserGroupQuery"
            resultType="java.lang.Integer">
        select COUNT(*)
        from user_group
        <where>
            <include refid="Base_Query_List"/>
        </where>
    </select>

    <!-- query -->
    <select id="query" parameterType="com.yunche.loan.domain.query.BaseQuery"
            resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>
        ,
        <include refid="Blob_Column_List"/>
        from user_group
        <where>
            <include refid="Base_Query_List"/>
        </where>
        ORDER BY gmt_create DESC
        limit #{startRow}, #{pageSize}
    </select>

    <!--getBaseUserGroupByEmployeeId-->
    <select id="getBaseUserGroupByEmployeeId" parameterType="java.lang.Long" resultMap="BaseResultMap">
        SELECT
                id, name
        FROM
                employee_rela_user_group AS a ,
                user_group AS b
        WHERE
                a.`user_group_id` = b.`id`
        AND     a.`employee_id` = #{employeeId,jdbcType=BIGINT}
        GROUP BY a.`user_group_id`
    </select>

    <!-- countListUserGroupByEmployeeIdAndAreaList -->
    <select id="countListUserGroupByEmployeeIdAndAreaList" parameterType="com.yunche.loan.domain.query.RelaQuery"
            resultType="java.lang.Integer">
        SELECT COUNT(*) FROM
        (
            SELECT b.*
            FROM
                    employee_rela_user_group AS a ,
                    user_group AS b ,
                    user_group_rela_area_auth AS c
            WHERE
                    a.`user_group_id` = b.`id` AND a.`user_group_id` = c.`user_group_id`
                    <if test="id != null">
                        AND a.employee_id = #{id}
                    </if>
                    <if test="status != null">
                        AND b.status = #{status}
                    </if>
                    <if test="areaIdList != null and areaIdList.size() > 0">
                        AND c.area_id IN
                        <foreach collection="areaIdList" item="item" open="(" close=")" separator=",">
                            #{item}
                        </foreach>
                    </if>
            GROUP BY b.`id`
        ) AS d
    </select>

    <!-- listUserGroupByEmployeeIdAndAreaList -->
    <select id="listUserGroupByEmployeeIdAndAreaList" parameterType="com.yunche.loan.domain.query.RelaQuery"
            resultMap="BaseResultMap">
        SELECT  b.*
        FROM
                employee_rela_user_group AS a ,
                user_group AS b ,
                user_group_rela_area_auth AS c
        WHERE
                a.`user_group_id` = b.`id` AND a.`user_group_id` = c.`user_group_id`
                <if test="id != null">
                    AND a.employee_id = #{id}
                </if>
                <if test="status != null">
                    AND b.status = #{status}
                </if>
                <if test="areaIdList != null and areaIdList.size() > 0">
                    AND c.area_id IN
                    <foreach collection="areaIdList" item="item" open="(" close=")" separator=",">
                        #{item}
                    </foreach>
                </if>
        GROUP BY b.`id`
        ORDER BY b.`id` ASC
        LIMIT #{startRow}, #{pageSize}
    </select>

    <!-- countListUserGroupByDepartmentId -->
    <select id="countListUserGroupByDepartmentId" parameterType="com.yunche.loan.domain.query.BaseQuery"
            resultType="java.lang.Integer">
        SELECT COUNT(*)
        FROM department_rela_user_group AS a
        JOIN user_group AS b
        ON a.`user_group_id` = b.`id`
        <if test="id != null">
            AND a.department_id = #{id}
        </if>
        <if test="status != null">
            AND b.status = #{status}
        </if>
    </select>

    <!-- listUserGroupByDepartmentId -->
    <select id="listUserGroupByDepartmentId" parameterType="com.yunche.loan.domain.query.BaseQuery"
            resultMap="BaseResultMap">
        SELECT b.*
        FROM department_rela_user_group AS a
        JOIN user_group AS b
        ON a.`user_group_id` = b.`id`
        <if test="id != null">
            AND a.department_id = #{id}
        </if>
        <if test="status != null">
            AND b.status = #{status}
        </if>
        ORDER BY b.`id` ASC
        LIMIT #{startRow}, #{pageSize}
    </select>

