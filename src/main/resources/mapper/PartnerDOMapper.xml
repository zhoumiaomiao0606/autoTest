        </where>
    </select>
    <!-- queryByPartnerName -->
    <select id="queryByPartnerName" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>
        from partner where name LIKE CONCAT('%',#{name},'%')
    </select>
    <!-- query -->
    <select id="query" parameterType="com.yunche.loan.domain.query.PartnerQuery"
            resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>
        from partner
        <where>
            <include refid="Base_Query_List"/>
        </where>
        ORDER BY gmt_create DESC
        limit #{startRow}, #{pageSize}
    </select>

